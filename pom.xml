  <version>1.0-SNAPSHOT</version>
  <name>proposalkeeper</name>
  <url>https://github.com/wmeints/app-insights-demo</url>
  <scm>
    <url>https://github.com/wmeints/app-insights-demo</url>
    <connection>scm:git://github.com/wmeints/app-insights-demo.git</connection>
    <developerConnection>scm:git:git@github.com:wmeints/app-insights-demo.git</developerConnection>
  </scm>
  <licenses>
    <license>
      <name>Apache-2.0</name>
      <url>https://opensource.org/licenses/Apache-2.0</url>
      <distribution>repo</distribution>
    </license>
  </licenses>
  <developers>
